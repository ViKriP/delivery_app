.general-cart-item.divider-lg-bottom.pt-0
  %table.table
    %tr
      %td.col-half
        %span.in-grey-600= t('name')
      %td.col-half
        %span.in-grey-900.font-16.fw-300= courier_list_xs.name
    %tr
      %td
        %span.in-grey-600= t('settings')
      %td
        = link_to t('show'), courier_path(courier_list_xs.id), class: "general-order-number"
        = link_to t('edit'), edit_courier_path(courier_list_xs.id), class: "general-order-number"
